{
  "name": "coeus-router",
  "version": "1.5.0",
  "description": "A tiny router component to build SPA with webpack and react",
  "main": "lib/Router.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "npm run clean:test && npm run clean:lib",
    "clean:test": "rimraf test/build test/routes",
    "clean:lib": "rimraf lib",
    "lint": "eslint src test --fix",
    "test": "scripts/build_test.sh && scripts/test.sh",
    "build:test": "scripts/build_test.sh",
    "build:lib": "babel src --out-dir lib",
    "check:src": "npm run lint && npm run test",
    "prepublish": "npm run clean && npm run check:src && npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChieveiT/coeus-router.git"
  },
  "keywords": [
    "redux",
    "react",
    "async",
    "promise",
    "tiny"
  ],
  "author": "ChieveiT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChieveiT/coeus-router/issues"
  },
  "homepage": "https://github.com/ChieveiT/coeus-router#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "coeux": "1.1.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.13.1",
    "eslint-config-reactjs": "^1.1.2",
    "eslint-plugin-objects": "^1.1.1",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.1",
    "expose-loader": "^0.7.1",
    "html-webpack-plugin": "^2.21.0",
    "js-beautify": "^1.6.3",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "mocha-loader": "webpack/mocha-loader",
    "mocha-phantomjs": "^4.1.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1",
    "yaml-loader": "^0.2.0",
    "coeux": "1.2.0",
    "coeus-utils": "1.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc.1",
    "webpack": "^1.13.1",
    "lodash": "^4.13.1",
    "coeux": "1.2.0",
    "coeus-utils": "1.0.0"
  }
}
